        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Login.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity android:name=".Project.Project_activity" />
        <activity android:name=".Defect.Defect_log" />
        <activity android:name=".Report.Severity" />
        <activity android:name=".Report.PriorityActivity" />
        <activity android:name=".Report.Typedefect" />
        <activity android:name=".Report.StatusDefect"></activity>
    </application>

</manifest>